{
    "name": "adsum/translation-bundle",
    "type": "symfony-bundle",
    "description": "Allows you to easily handle twig and javascript translations",
    "keywords": ["translations", "twig", "javascript"],
    "require": {
        "php": ">=5.3.2",
        "jms/translation-bundle": "~1.0",
        "willdurand/js-translation-bundle": "~2.2",
        "elao/form-translation-bundle": "1.1.*",
        "jms/di-extra-bundle": "~1.0"
    },
    "require-dev": {
        "symfony/yaml": "*",
        "symfony/browser-kit": "*",
        "symfony/class-loader": "*",
        "symfony/finder": "*",
        "symfony/css-selector": "*",
        "symfony/process": "*",
        "doctrine/doctrine-bundle": "*",
        "symfony/twig-bundle": "*",
        "doctrine/orm": "*",
        "symfony/form": "*",
        "symfony/validator": "*",
        "symfony/stopwatch": "*"
    },
    "autoload": {
        "psr-0": {
            "Adsum\\TranslationBundle": ""
        }
    },
    "target-dir": "Adsum/TranslationBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.13-dev"
        }
    }
}